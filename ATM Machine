#include <stdio.h>
int main() {
	// your code goes here
    int T;
    scanf("%d",&T);
    while(T--){
        int N,K;
        scanf("%d %d",&N,&K);
        int A[N];
        for(int i=0;i<N;i++){
            scanf("%d",&A[i]);
        }
        for(int i=0;i<N;i++){
            if(A[i]<=K){
                K=K-A[i];
                printf("1");
            } else{
                printf("0");
            }
        }
        printf("\n");
    }
}
