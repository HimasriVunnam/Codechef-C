#include <stdio.h>
#include<stdbool.h>
int main() {
	// your code goes here
    int T;
    scanf("%d",&T);
    while(T--){
        int N;
        char S[101];
        scanf("%d",&N);
        scanf("%s",S);
        int c_count=0;
        bool pronounce=false;
        for (int i=0;i<N;i++){
            if (S[i]=='a' || S[i]=='e' || S[i]=='i' || S[i]=='o' || S[i]=='u') {
                c_count=0;
            }
            else {
                c_count++;
                if (c_count>=4) {
                    pronounce=true;
                    break;
                }
            }
        }
        if(pronounce) {
            printf("NO\n");
        } else {
            printf("YES\n");
        }
    }
}

